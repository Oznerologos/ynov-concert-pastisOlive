oken', token);\r\n        \r\n            _setAxiosToken(token);\r\n\r\n            return true;\r\n        });\r\n}\r\n\r\nfunction init() {\r\n    const token = window.localStorage.getItem('authToken');\r\n    if(token) {\r\n        const jwtData = jwtDecode(token);\r\n\r\n        if(jwtData.exp * 1000 > new Date().getTime()) {\r\n            _setAxiosToken(token);\r\n        }\r\n    }\r\n}\r\n\r\nfunction isAuth() {\r\n    const token = window.localStorage.getItem('authToken');\r\n    if(token) {\r\n        const jwtData = jwtDecode(token);\r\n\r\n        if(jwtData.exp * 1000 > new Date().getTime()) {\r\n            return true\r\n        }\r\n        return false\r\n    }\r\n    return false\r\n} \r\n\r\nfunction _setAxiosToken(token) {\r\n    axios.defaults.headers[\"Authorization\"] = \"Bearer \" + token;\r\n}\r\n\r\nexport default {\r\n    auth,\r\n    logout,\r\n    init,\r\n    isAuth\r\n}",{"ruleId":"157","replacedBy":"158"},{"ruleId":"159","replacedBy":"160"},{"ruleId":"161","severity":1,"message":"162","line":11,"column":17,"nodeType":"163","endLine":11,"endColumn":34},{"ruleId":"164","severity":1,"message":"165","line":2,"column":8,"nodeType":"166","messageId":"167","endLine":2,"endColumn":15},{"ruleId":"164","severity":1,"message":"168","line":3,"column":8,"nodeType":"166","messageId":"167","endLine":3,"endColumn":16},{"ruleId":"164","severity":1,"message":"169","line":4,"column":8,"nodeType":"166","messageId":"167","endLine":4,"endColumn":16},{"ruleId":"170","severity":1,"message":"171","line":50,"column":20,"nodeType":"163","endLine":50,"endColumn":44},{"ruleId":"164","severity":1,"message":"172","line":1,"column":16,"nodeType":"166","messageId":"167","endLine":1,"endColumn":24},{"ruleId":"164","severity":1,"message":"173","line":10,"column":8,"nodeType":"166","messageId":"167","endLine":10,"endColumn":19},{"ruleId":"164","severity":1,"message":"174","line":11,"column":8,"nodeType":"166","messageId":"167","endLine":11,"endColumn":17},{"ruleId":"175