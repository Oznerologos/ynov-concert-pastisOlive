his.setState({price: newValue})\n\n    if(this.state.selected === false){\n        this.setState({selected:true});\n        newValue = parseInt(initialValue) + parseInt(this.props.price)\n        this.setState({id: this.props.id})\n        this.setState({price: newValue})\n        return\n    }\n    this.setState({selected:false});\n    newValue = parseInt(initialValue) - parseInt(this.props.price)\n    this.setState({id: this.props.id})\n    this.setState({price: newValue})\n\n    \n    this.context.setSeats([{\n        id: this.state.id,\n        price:this.state.price,\n        selected:this.state.selected,\n}])\nconsole.log(this.context);\n*/}\n}\n\n    render() {\n\n        return(\n                <Tooltip title={this.props.price} placement=\"top-end\" key={`tool-${this.props.seatKey}`} arrow>\n                    <img src={this.state.seat ?? this.props.src} onClick={() => this.onClick(this.props.selected)} selected={this.state.selected} onMouseEnter={() => this.onHover(this.props.src)} onMouseLeave={() => this.onLeave(this.props.seat)} price={this.props.price} id={this.props.id} alt={this.props.alt} width={this.props.width} className={this.props.className}/>\n                </Tooltip>\n        )\n    };\n};\n\n\nexport default Seat;",{"ruleId":"188","replacedBy":"189"},{"ruleId":"190","replacedBy":"191"},{"ruleId":"192","severity":1,"message":"193","line":11,"column":17,"nodeType":"194","endLine":11,"endColumn":34},{"ruleId":"195","severity":1,"message":"196","line":50,"column":1,"nodeType":"197","endLine":55,"endColumn":2},{"ruleId":"198","severity":1,"message":"199","line":2,"column":8,"nodeType":"200","messageId":"201","endLine":2,"endColumn":15},{"ruleId":"198","severity":1,"message":"202","line":3,"column":8,"nodeType":"200","messageId":"201","endLine":3,"endColumn":16},{"ruleId":"198","severity":1,"message":"203","line":4,"column":8,"nodeType":"200","messageId":"201","endLine":4,"endColumn":16},{"ruleId":"204","severity":1,"message":"205","line":50,"co